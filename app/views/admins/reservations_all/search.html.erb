
<%= form_with model:@reservation, url:admins_reservations_all_search_path, method: :get, local: true do |f|%>
<%= f.select :keyword,@hash %>
<%= f.submit "確認する", class: 'btn btn-primary' %>
<% end %>
<table class = "table">
    <thead>
        <tr>
            <th>予約日</th>
            <th>タイトル</th>
            <th>担当者</th>
            <th>受付日</th>
        </tr>
    </thead>

    <tbody>
        <% @reservations.each do |reservation| %>
        <tr>
            <td><%= reservation.start_time.strftime('%Y/%m/%d %H時%M分') %>~<%= reservation.end_time.strftime('%H時%M分') %></td>
            <td><%= reservation.title %></td>
            <td><%= reservation.staff %></td>
            <td><%= reservation.created_at.strftime('%Y/%m/%d %H時%M分') %></td>
        </tr>
        <% end %>
    </tbody>
</table>